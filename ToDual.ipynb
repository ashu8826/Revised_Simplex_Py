      "[[1 2 1]\n",
      " [1 3 5]\n",
      " [1 0 0]\n",
      " [0 1 0]\n",
      " [0 0 1]]\n",
      "[6, 8, 0, 0, 0]\n",
      "[10, 25, 35]\n"
     ]
    }
   ],
   "source": [
    "print(Ad)\n",
    "print(bd)\n",
    "print(cd)"
   ]
