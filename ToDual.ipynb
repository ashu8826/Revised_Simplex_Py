   "source": [
    "import numpy as np\n",
    "#primal problem\n",
    "A = [[1,1,1,0,0],\n",
    "     [2,3,0,1,0],\n",
    "     [1,5,0,0,1]]\n",
    "b = [10,25,35]\n",
    "c = [6,8,0,0,0]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "#dual problem \n",
    "Ad = np.transpose(A)\n",
    "bd = c\n",
    "cd = b"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
